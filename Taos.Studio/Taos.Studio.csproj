    <EmbeddedResource Include="Resources\resultset_next.png" />
    <EmbeddedResource Include="Resources\table.png" />
    <EmbeddedResource Include="Resources\table_delete.png" />
    <EmbeddedResource Include="Resources\table_lightning.png" />
    <EmbeddedResource Include="Resources\table_save.png" />
    <EmbeddedResource Include="Resources\table_sort.png" />
