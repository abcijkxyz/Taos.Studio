    <EmbeddedResource Include="ICSharpCode.TextEditor\Resources\RightArrow.cur" />
    <EmbeddedResource Include="ICSharpCode.TextEditor\Resources\SQL-Mode.xshd" />
    <EmbeddedResource Include="ICSharpCode.TextEditor\Resources\SyntaxModes.xml" />
    <EmbeddedResource Include="ICSharpCode.TextEditor\Resources\TextEditorControl.bmp" />
    <EmbeddedResource Include="Resources\application_put.png" />
    <EmbeddedResource Include="Resources\application_view_tile.png" />
    <EmbeddedResource Include="Resources\arrow_refresh.png" />
    <EmbeddedResource Include="Resources\brick.png" />
    <EmbeddedResource Include="Resources\bug_link.png" />
    <EmbeddedResource Include="Resources\compress.png" />
    <EmbeddedResource Include="Resources\database.png" />
    <EmbeddedResource Include="Resources\database_connect.png" />
